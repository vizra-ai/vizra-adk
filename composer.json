{
  "name": "vizra/vizra-adk",
  "version": "0.0.40",
  "description": "Vizra Agent Development Kit - A comprehensive Laravel package for building intelligent AI agents.",
  "keywords": [
    "laravel",
    "ai",
    "agents",
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "gpt",
    "claude",
    "ai-agents",
    "ai-tools",
    "agent-framework",
    "workflow",
    "rag",
    "vector-memory",
    "mcp",
    "model-context-protocol",
    "prism-php",
    "embedding",
    "ai-development-kit",
    "intelligent-agents",
    "conversational-ai",
    "ai-workflows",
    "memory-persistence",
    "meilisearch"
  ],
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron Lumsden",
      "email": "aaronlumsden@me.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "prism-php/prism": "^0.92.0",
    "league/csv": "^9.23",
    "livewire/livewire": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Vizra\\VizraADK\\": "src/",
      "Vizra\\VizraADK\\Examples\\": "examples/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vizra\\VizraADK\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vizra\\VizraADK\\Providers\\AgentServiceProvider"
      ],
      "aliases": {
        "Agent": "Vizra\\VizraADK\\Facades\\Agent",
        "Workflow": "Vizra\\VizraADK\\Facades\\Workflow"
      }
    }
  },
  "scripts": {
    "test": "pest",
    "test-coverage": "pest --coverage"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
